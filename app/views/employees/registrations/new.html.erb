<div class="sessions-new">
  <h4>従業員 新規登録</h4>

  <p>従業員登録画面</p>
  <p>※まだ未実装です</p>
</div>
<%
=begin%>
 <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>

  <%= render "employees/shared/error_messages", resource: resource %> 

  <div class="field">
    <%= f.label :name, "名前" %><br />
    <%= f.text_field :name, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :email, "ID（〇〇@〜〜）" %><br />
    <%= f.text_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password, "パスワード（6-20字）" %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation, "パスワード（確認用）" %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>

  <div class="actions">
    <%= f.submit "新規登録" %>
  </div>
<% end %> 
<%
=end%>

