<div id="<%= dom_id schedule %>">
  <p>
    <strong>Place:</strong>
    <%= schedule.place %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= schedule.date %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= schedule.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= schedule.end_time %>
  </p>

  <p>
    <strong>Memo:</strong>
    <%= schedule.memo %>
  </p>

  <p>
    <strong>Owner:</strong>
    <%= schedule.owner_id %>
  </p>

</div>
